<%- include('../partials/header', { title: title, nav: nav }) %>

<main>
  <% if (locals.notice) { %>
    <div class="flash notice"><%= notice %></div>
  <% } %>

  <% if (errors && errors.length > 0) { %>
    <div class="errors">
      <% errors.forEach(error => { %>
        <p><%= error.msg %></p>
      <% }) %>
    </div>
  <% } %>

  <h1>You're logged in</h1>
</main>

<%- include('../partials/footer') %>
